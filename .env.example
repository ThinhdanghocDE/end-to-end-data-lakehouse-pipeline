# ==============================================
# E-commerce Data Platform - Environment Variables
# ==============================================
# Copy this file to .env and update with your values
# DO NOT commit .env to version control

# PostgreSQL Configuration
POSTGRES_USER=ecommerce
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=ecommerce_db
PG_HOST=localhost
PG_PORT=5433

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9094
KAFKA_INTERNAL_BOOTSTRAP_SERVERS=kafka:29092

# ClickHouse Configuration
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=8124
CLICKHOUSE_DB=ecommerce_dw
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# MinIO (S3-compatible Data Lake)
MINIO_ENDPOINT=http://localhost:9002
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
BRONZE_BUCKET=bronze
SILVER_BUCKET=silver
GOLD_BUCKET=gold

# Superset
SUPERSET_SECRET_KEY=your_superset_secret_key
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=admin

# Debezium
DEBEZIUM_URL=http://localhost:8084
DEBEZIUM_CONNECTOR_NAME=ecommerce-connector

# Data Generator Settings
ORDERS_PER_MINUTE=10
UPDATE_PROBABILITY=0.3
